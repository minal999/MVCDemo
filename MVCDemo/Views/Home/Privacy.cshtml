@{
    ViewData["Title"] = "Sample Demo";
}
@*<h4>@ViewData["Title"]</h4>*@

<h4>Working with ViewData</h4>
<p>Name:
    @ViewData["name"]<br />
    Email: @ViewData["email"]<br />
    Gender: @ViewData["gender"]
</p>
<p>
    <h6>List of Cricketers </h6>
    <ul>
        @foreach (string item in (IEnumerable<string>)ViewData["Cricketers"])
        {
            <li>
                @item
            </li>
        }
    </ul>
</p>
<h4>Working with ViewBag</h4>
<p>Name: @ViewBag.name<br />
    Email: 
    @ViewBag.email<br />
    Gender: @ViewBag.gender
</p>
<p>
    <h6>List of Cricketers </h6>
    <ul>
        @foreach (string item in ViewBag.Cricketers)
        {
            <li>
                @item
            </li>
        }
    </ul>
</p>

